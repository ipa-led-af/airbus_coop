<?xml version="1.0"?>

<plugin>
    <import module="omni_gui.plugin" class="PluginOmniV2"/>
    <description>A Python plugin for displaying omni in cobot_gui.</description>
    <setup>
        <icon>${omni_gui}/resources/images/icon_omni.png</icon>
        <access-rights>operator</access-rights>
        <parameters>
            <param name="yaml" value="${omni_gui}/resources/yaml/sara.rviz"/>
            <param name="linear_speed" value="0.5"/>
            <param name="angular_speed" value="0.5"/>
        </parameters>
    </setup>
</plugin>

