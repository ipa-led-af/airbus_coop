    <td ALIGN="CENTER" BGCOLOR="#FF4500" COLSPAN="1">Python executable generated</td>
  </tr>
  <tr>
    <td ALIGN="CENTER" BGCOLOR="#FFFFFF" COLSPAN="1">irt_sensor.sh</td>
  </tr>
  <tr>
    <td ALIGN="CENTER" BGCOLOR="#FFFFFF" COLSPAN="1">irt_sensor_old.sh</td>
  </tr>
</table>
>];
